// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("WAREHOUSE_DATABASE_URL")
}

model account_versions {
  id           Int       @id @default(autoincrement())
  member_id    Int?
  account_id    Int?
  reason       Int?
  balance      Decimal?
  locked       Decimal?
  fee          Decimal?
  amount       Decimal?
  modifiable_id Int?
  modifiable_type String?
  created_at    DateTime?  @default(now())
  updated_at    DateTime?  @default(now())
  currency   Int?
  fun      Int?
}

model accounting_events {
  id           Int       @id @default(autoincrement())
  event_code   String
  type         String
  details      Json
  occurred_at  Int       @default(0)
  created_at   Int       @default(0)
  account_version_ids Json
}

model jobs {
  id           Int       @id @default(autoincrement())
  table_name   String    @unique
  sync_id      Int
  parsed_id    Int
  created_at   Int       @default(0)
  updated_at   Int       @default(0)
}

model members {
  id           Int       @id @default(autoincrement())
  email        String    @unique
}

model accounts {
  id           Int       @id @default(autoincrement())
  member_id    Int
  account_id   Int
  currency     Int
  balance      Decimal
  locked       Decimal
  created_at   Int       @default(0)
  updated_at   Int       @default(0)
}

model accounts_2023_04_01 {
  id           Int       @id @default(autoincrement())
  member_id    Int
  account_id   Int
  currency     Int
  balance      Decimal
  locked       Decimal
  created_at   Int       @default(0)
  updated_at   Int       @default(0)
}

model accounts_2024_02_01 {
  id           Int       @id @default(autoincrement())
  member_id    Int
  account_id   Int
  currency     Int
  balance      Decimal
  locked       Decimal
  created_at   Int       @default(0)
  updated_at   Int       @default(0)
}

model version_times {
  id           Int       @id @default(autoincrement())
  account_version_id Int
  date         String
  unix_timestamp Int
}

model reports {
  id           Int       @id @default(autoincrement())
  name         String
  data         Json
  created_at   Int       @default(0)
}

model referral_commissions {
  id                       Int       @id @default(autoincrement())
  referred_by_member_id    Int?
  trade_member_id          Int?
  market                   Int?
  currency                 Int?
  amount                   Decimal?
  state                    String?
  created_at               DateTime?  @default(now())
  deposited_at             DateTime?  @default(now())
}

model orders {
  id                  Int       @id @default(autoincrement())
  ask                 Int
  bid                 Int
  price               Decimal?
  origin_volume       Decimal?
  type                String?
  member_id           Int
  created_at          Int       @default(now())
  updated_at          Int       @default(now())
}

model trades {
  id                  Int       @id @default(autoincrement())
  currency            Int
  ask_id              Int
  bid_id              Int
  ask_member_id       Int
  bid_member_id       Int
  created_at          Int       @default(now())
}