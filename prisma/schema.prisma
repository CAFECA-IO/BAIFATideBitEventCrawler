// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("WAREHOUSE_DATABASE_URL")
}

model account_versions {
  id           Int       @id @default(autoincrement())
  member_id    Int?
  account_id    Int?
  reason       Int?
  balance      Decimal?
  locked       Decimal?
  fee          Decimal?
  amount       Decimal?
  modifiable_id Int?
  modifiable_type String?
  created_at    DateTime?  @default(now())
  updated_at    DateTime?  @default(now())
  currency   Int?
  fun      Int?
}

model accounting_events {
  id           Int       @id @default(autoincrement())
  event_code   String
  type         String
  details      Json
  occurred_at  Int       @default(0)
  created_at   Int       @default(0)
  account_version_ids Json
}

model jobs {
  id           Int       @id @default(autoincrement())
  table_name   String    @unique
  sync_id      Int
  parsed_id    Int
  created_at   Int       @default(0)
  updated_at   Int       @default(0)
}

model members {
  id           Int       @id @default(autoincrement())
  email        String    @unique
}

model accounts {
  id           Int       @id @default(autoincrement())
  member_id    Int
  currency     Int
  balance      Decimal
  locked       Decimal
  created_at   Int       @default(0)
  updated_at   Int       @default(0)
}

model version_time {
  id           Int       @id @default(autoincrement())
  account_version_id Int
  date         String
  unix_timestamp Int
}